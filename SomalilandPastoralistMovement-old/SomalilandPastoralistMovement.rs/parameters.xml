<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

			
	<parameter name="startMonth" displayName="Start of Simulation Month" type="int" 
		defaultValue="1" 
		isReadOnly="true" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
	/>
	
	<parameter name="samplingPercent" displayName="Sampling percent per admin region" type="int" 
		defaultValue="12" 
		isReadOnly="true" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
		/>

	<parameter name="startYear" displayName="Start Year" type="int" 
		defaultValue="2008" 
		isReadOnly="true" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"/>
	
	<parameter name="endYear" displayName="End Year" type="int" 
		defaultValue="2014" 
		isReadOnly="true" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"/>	
		
	<parameter name="minScoutingRange" displayName="Minimum scouting range (km) for nearby locations" type="int" 
		defaultValue="15" 
		isReadOnly="true" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
		/>	
		
	<parameter name="maxScoutingRange" displayName="Maximum scouting range (km) for nearby locations" type="int" 
		defaultValue="30" 
		isReadOnly="true" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
		/>		
		
		
		
		
		
		
		
		
		
	<parameter name="randomSeed" displayName="Default Random Seed" type="int" 
		defaultValue="__NULL__"
		isReadOnly="false" 
		converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
		/>
		
</parameters>
